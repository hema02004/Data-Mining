        "def manhattan_distance(x, y):\n",
        "    return np.sum(np.abs(x - y))\n",
        "\n",
        "# Example usage\n",
        "point1 = np.array([1, 2, 3])\n",
        "point2 = np.array([4, 5, 6])\n",
        "print(\"Manhattan Distance:\", manhattan_distance(point1, point2))"
